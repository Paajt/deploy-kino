{
  "name": "kino-bio-projekt",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "start": "concurrently \"npm run dev:vite\" \"npm run dev:node\"",
    "dev:vite": "vite",
    "dev:node": "nodemon server.js",
    "test": "cross-env NODE_NO_WARNINGS=1 jest --verbose"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "globals": {},
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "supertest": "^7.0.0",
    "vite": "^6.0.1",
    "vite-plugin-sass": "^0.1.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "marked": "^15.0.6",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9"
  }
}
